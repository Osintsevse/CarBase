<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="2" author="Stas">
        <createTable tableName="models">
            <column name="model_id" type="SERIAL" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="model_name" type="TEXT">
                <constraints nullable="false"/>
            </column>
            <column name="vendor_id" type="SERIAL">
                <constraints nullable="false"
                             references="vendors(vendor_id)"
                             foreignKeyName="FK_models_vendors"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>